{% extends 'base.html.twig' %}

{% block title %}Modification d'une reservation{% endblock %}
 {% block monTitre %}Modifier la reservation{% endblock %}
{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.Utilisateurs, {'label' : 'nom utilisateur '}) }}
    {{ form_row(form.Salles, {'label' : 'salle'}) }}
    {{ form_row(form.date) }}
    {{ form_row(form.heure_deb) }}
    {{ form_row(form.heure_fin) }}
    {{ form_row(form.nbr_personnes) }}
    {{ form_row(form.Extras, {'label' : 'extra'}) }}


    <input type="submit" class="btn btn-primary" value="Modifier" >

    {{ form_end(form) }}
{% endblock %}
